{
    "name": "fast-react-calendar",
    "version": "1.0.0-alpha.9",
    "description": "A fast and flexible calendar component for React, only for MUI.",
    "author": "Mattis",
    "license": "MIT",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        "import": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "require": {
            "types": "./dist/index.d.cts",
            "require": "./dist/index.cjs"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup src/index.tsx --format cjs,esm --dts --clean --sourcemap",
        "prepublishOnly": "npm run build",
        "lint": "eslint . --ext .ts,.tsx",
        "prepare": "husky"
    },
    "keywords": [
        "calendar",
        "react",
        "component",
        "mui",
        "date",
        "events"
    ],
    "type": "module",
    "dependencies": {
        "@mui/icons-material": "^7.1.1",
        "@mui/x-date-pickers": "^8.5.3",
        "date-fns": "^4.1.0"
    },
    "peerDependencies": {
        "@emotion/react": "^11.11.0",
        "@emotion/styled": "^11.11.0",
        "@mui/material": "^5.15.0 || ^7.0.0",
        "react": "^18.0.0 || ^19.0.0",
        "react-dom": "^18.0.0 || ^19.0.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.29.0",
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "eslint": "^9.29.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-prettier": "^5.5.0",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "globals": "^16.2.0",
        "husky": "^9.1.7",
        "lint-staged": "^16.1.2",
        "prettier": "^3.6.0",
        "tsup": "^8.5.0",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.35.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Mattis44/fast-react-calendar.git"
    },
    "lint-staged": {
        "*.ts?(x)": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.json": [
            "prettier --write"
        ]
    },
    "bugs": {
        "url": "https://github.com/Mattis44/fast-react-calendar/issues"
    },
    "homepage": "https://github.com/Mattis44/fast-react-calendar#readme"
}
